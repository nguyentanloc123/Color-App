<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="10dp"
    android:paddingTop="10dp"
    tools:context="com.tanloc2017.appmau.MainEXtract">


    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="-9dp"
        android:layout_marginTop="-9dp">

        <com.makeramen.roundedimageview.RoundedImageView xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/img"
            android:layout_width="match_parent"
            android:layout_height="413dp"


            android:background="#d97077"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:riv_mutate_background="true"
            app:riv_oval="false" />


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="2dp"


            android:layout_marginBottom="76dp"
            android:background="#707070"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="2dp"

            android:layout_marginTop="76dp"
            android:background="#707070"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <mehdi.sakout.fancybuttons.FancyButton
            android:id="@+id/btn_2"
            android:layout_width="50dp"
            android:layout_height="50dp"

            android:layout_alignParentEnd="true"


            android:layout_marginBottom="16dp"
            android:layout_marginEnd="44dp"
            android:text=""
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btnconvert"
            fancy:fb_defaultColor="#f36f82"
            fancy:fb_focusColor="#c76161"
            fancy:fb_iconResource="@drawable/papers"
            fancy:fb_radius="1000000dp"></mehdi.sakout.fancybuttons.FancyButton>


        <mehdi.sakout.fancybuttons.FancyButton
            android:id="@+id/btnconvert"
            android:layout_width="50dp"
            android:layout_height="50dp"

            android:layout_alignParentEnd="true"


            android:layout_marginBottom="16dp"
            android:layout_marginEnd="24dp"
            android:text=""
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            fancy:fb_defaultColor="#f54841"
            fancy:fb_focusColor="#c76161"
            fancy:fb_iconResource="@drawable/converticon"
            fancy:fb_radius="1000000dp"></mehdi.sakout.fancybuttons.FancyButton>


        <mehdi.sakout.fancybuttons.FancyButton
            android:id="@+id/bt_load"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"


            android:layout_marginEnd="44dp"
            android:layout_marginStart="164dp"
            android:text=""
            app:layout_constraintBottom_toBottomOf="@+id/btn_2"
            app:layout_constraintEnd_toStartOf="@+id/btn_2"
            app:layout_constraintHorizontal_bias="0.875"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btn_2"
            app:layout_constraintVertical_bias="0.0"
            fancy:fb_defaultColor="#f36f82"
            fancy:fb_focusColor="#c76161"
            fancy:fb_iconResource="@drawable/camera"
            fancy:fb_radius="1000000dp"></mehdi.sakout.fancybuttons.FancyButton>


        <mehdi.sakout.fancybuttons.FancyButton
            android:id="@+id/bt_home"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/btn_2"
            android:layout_marginBottom="9dp"
            android:layout_marginEnd="44dp"
            android:layout_marginStart="28dp"
            android:text=""
            app:layout_constraintBottom_toBottomOf="@+id/bt_load"
            app:layout_constraintEnd_toStartOf="@+id/bt_load"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/bt_load"
            app:layout_constraintVertical_bias="0.0"
            fancy:fb_defaultColor="#f36f82"
            fancy:fb_focusColor="#c76161"
            fancy:fb_iconResource="@drawable/home"
            fancy:fb_radius="1000000dp"></mehdi.sakout.fancybuttons.FancyButton>

        <Button
            android:id="@+id/button"
            android:layout_width="76dp"
            android:layout_height="32dp"


            android:layout_alignParentStart="true"

            android:layout_marginStart="-8dp"
            android:layout_marginTop="32dp"
            android:background="@android:drawable/gallery_thumb"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/txtrbg"
            android:layout_width="88dp"
            android:layout_height="34dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="8dp"
            android:layout_marginTop="32dp"
            android:text=" R: 255   B: 155
        G:150"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/button"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/txthsl"
            android:layout_width="87dp"
            android:layout_height="31dp"


            android:layout_marginStart="8dp"
            android:layout_marginTop="32dp"
            android:text="h:130*    s=120
        v=155"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/txtrbg"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/txthex"
            android:layout_width="77.5dp"
            android:layout_height="32dp"

            android:layout_marginStart="12dp"

            android:layout_marginTop="32dp"
            android:text="     #fffff"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/txthsl"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/txtname"
            android:layout_width="181dp"
            android:layout_height="0dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="152dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/cl"
            android:layout_width="69dp"
            android:layout_height="0dp"

            android:layout_alignParentEnd="true"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/alike"
            android:text="Color"
            android:textAlignment="center"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/h"
            android:layout_width="69dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"

            android:layout_marginStart="28dp"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/alike"
            android:text="Hex"
            android:textAlignment="center"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/hs"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/hs"
            android:layout_width="71dp"
            android:layout_height="26dp"


            android:layout_alignTop="@+id/cl"

            android:layout_marginStart="48dp"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/alike"
            android:text="HSL"
            android:textAlignment="center"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/rbg"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/rbg"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="36dp"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/alike"
            android:text="RBG"
            android:textAlignment="center"
            android:textSize="17dp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/cl"
            app:layout_constraintTop_toTopOf="parent" />

